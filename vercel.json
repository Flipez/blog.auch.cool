{
  "build": {
    "env": {
      "HUGO_VERSION": "0.147.9"
    }
  },
  "redirects": [
    {
      "source": "/spezi",
      "has": [
        {
          "type": "host",
          "value": "auch.cool"
        }
      ],
      "destination": "https://spezi.auch.cool/"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "enten.auch.cool"
        }
      ],
      "destination": "https://auch.cool/enten/"
    }
  ]
}

