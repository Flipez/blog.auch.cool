{
  "build": {
    "env": {
      "HUGO_VERSION": "0.124.1"
    }
  },
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "spezi.auch.cool"
        }
      ],
      "destination": "https://auch.cool/spezi/"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "enten.auch.cool"
        }
      ],
      "destination": "https://auch.cool/enten/"
    }
  ]
}

